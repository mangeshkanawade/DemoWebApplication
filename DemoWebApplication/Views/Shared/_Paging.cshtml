@{
    decimal TotalPages = 1;
    if (Model.PageSize > 0) {
        TotalPages = Math.Ceiling((decimal)Model.TotalRecords / Model.PageSize);
    } else {
        TotalPages = Math.Ceiling((decimal)Model.TotalRecords / 10);
    }
    for (int i = 1; i <= TotalPages; i++) {
        <li>
            @i
        </li>
    }
}